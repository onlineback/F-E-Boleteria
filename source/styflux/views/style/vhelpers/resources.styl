/**
* contenedores
**/
.upload-box
	box-sizing border-box
	background #f2f2f2
	padding 2%
	margin-right 24px
	.dotBox
		border 2px dotted #b3b3b3
		min-height 120px
		padding 12px
		position relative
		text-align center
