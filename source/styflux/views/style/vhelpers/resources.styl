/*
*	Decors
*/
.decorOpt-label
	display inline-block
	*display inline
	font-family family4
	font-size 13px
	line-height 14px
	padding-left 22px
	position relative
	user-select none
	&.active
		.glyph-check, .glyph-radio
			display block
	input[type="radio"]
		display none
	.decorOptRad
		background #FFF
		border 1px solid #a3a3a3
		border-radius 10px
		color #fff
		font-size 10px
		height 10px
		left 0
		margin-top -6px
		position absolute
		top 50%
		width 10px
		transition all .3s ease
	&.fix2quad
		.decorOptRad
			border-radius 0px
	.glyph-radio
		display none
		font-size 8px
		height 10px
		line-height 10px
		margin 0 auto
		text-align center
		//text-indent 1px
		width 10px
		transition all 1s ease
	.glyph-check
		display none
		font-size 7px
		line-height 10px
		text-align center
		text-indent 1px
		width 10px
	.crclIcon
		background #f2f2f2
		border-radius 18px
		font-size 14px
		line-height 18px
		padding 6px
		text-align center
		vertical-align middle
		width 18px
		&.glyph-sell
			padding 6px 5px 6px 7px
			text-align left
		&.glyph-rental
			font-size 13px
			//text-indent 1px
		&.glyph-activityGame
			font-size 13px
.ctnLbl-block
	.decorOpt-label
		display block
		& + .decorOpt-label
			margin-top 10px
.brdr-5
