.imagen-principal
	float left
	width 100%
	background #fff
	position absolute
.sidebar-info
	float left
	position relative
	width 290px
	margin-bottom 35px
	.imgCtn
		width 100%
		text-align center
		position relative
		.centerImg
			display inline-block
			position relative
		img
			box-shadow 0 29px 27px -23px #2c2c2c
			min-width 130px
		.icExpandImg
			position absolute
			top 5px
			right 5px
	.imgSelector
		margin-top 30px
		img
			float left
			margin 0 0 7px 8px
