		width 85%
	.icUpPicture
		position absolute
		top 50%
		left 50%
		margin -37px 0 0 -49px
.grid-filter
	margin 0 auto 8px
	*zoom 1
	&:before,&:after
		display table
		content ""
		line-height 0
	&:after
		clear both
	.gfilter-left
		float left
	.gfilter-right
		float right
	.form-control
		border-radius 3px !important
		font-size 12px !important
		height 30px
		padding 6px 8px
		width 300px
/*
*	Rows Separates
*/
.bxs-20
	margin 0 -20px
	.column-8,.column-7, .column-4,.column-5
		padding 0 20px
.bxs-15
	margin 0 -15px
	.column-8,.column-7, .column-4,.column-5
		padding 0 15px
.bxs-10
	margin 0 -10px
	.column-9, .column-3, .column-6
		padding 0 10px
.bxsp-4
	margin 0 -4%
	.column-6
		padding 0 4%
/*
*	Calendar
*/
.custom-calendar
	font-family family7
	margin-top 10px
	text-align center
	*zoom 1
	&:before,&:after
		display table
		content ""
		line-height 0
	&:after
		clear both
	.calDatePicker
		.bxcal-desc
			border none
			border-radius 0
			color #fff
			font-size 42px
			font-family family7
			line-height 43px
			text-align center
			opacity 0
		.month-dp
			color #fff
			float left
			font-family family5
			font-size 12px
			margin 7px auto 0
			position absolute
			width 100%
			z-index 1
		.showDayNum
			color #fff
			font-size 42px
			font-family family7
			height 69px
			line-height 83px
			position absolute
			text-align center
			width 100%
			background #f2f2f2
	.box-calendar
		float left
		position relative
		width 80px
		.bxcal-desc
			transition all 3s ease
		& + .box-calendar
			margin-left 14px
		&.active, &:hover
			.bxcal-desc, .showDayNum
				background #00bcba
				transition all 2s ease
			.bxcald-moth, .bxcald-day
				color #FFF
	.bxcal-desc
		background #f2f2f2
		padding 20px 0 6px
		position relative
		margin-bottom 2px
	.bxcal-slct
		background #f2f2f2
		box-sizing border-box
		color #808080
		font-family family4
		font-size 12px
		height 24px
		line-height 22px
		text-align center
	.bxcald-moth
		color #808080
		font-family family5
		font-size 12px
		left 0
		position absolute
		top 4px
		width 100%
	.bxcald-day
		color #808080
		display block
		font-size 42px
	select
		background #f2f2f2
		border none
		color #808080
		font-size 12px
		height 20px
		padding 2px
/**
* Days picker
**/
.daysPicker
	margin-top 10px
	width 330px
	label
		background #f2f2f2
		color #6f6f6f
		display block
		float left
		height 30px
		line-height 30px
		margin 0 5px 10px 0
		padding 0 !important
		text-align center
		transition all .5s ease 0s
		width 60px
		&.act
			background #00bcbb
			color #fff
		&:hover
			opacity .8
		input[type="checkbox"]
			display none
/*
*	Rating
*/
.ratingDetail
	.rtngdet-stats
		margin-top 8px
		margin-bottom 5px
		*zoom 1
		&:before,&:after
			display table
			content ""
			line-height 0
		&:after
			clear both
		&:first-child
			margin-top 0
	.rtngdet-starts
		float left
		width 110px
	.rtngdet-hr
		border-bottom 1px solid #f1f1f1
		float left
		height 13px
		margin 0 32px
		width 60px
	.rtngdet-rat
		color #999
		float left
		font-family family5
		font-size 12px
		line-height 12px
		padding-top 2px
.rtngdet-starts
	font-size 14px
	&.start-bad
		color #ef1688
	&.start-regular
		color #f9cc3d
	&.start-good
		color #93db2c
	&.start-excellent
		color #2add66
	.glyph-favorites
		margin-left 6px
		vertical-align top
		&:first-child
			margin 0
